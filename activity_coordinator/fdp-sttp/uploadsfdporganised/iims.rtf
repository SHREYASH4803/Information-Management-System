<script>  \par
    function downloadCSV(csv, filename) \{  \par
        var csvFile;  \par
        var downloadLink;  \par
        csvFile = new Blob([csv], \{type: 'text/csv'\});  \par
        downloadLink = document.createElement("a");  \par
        downloadLink.download = filename;  \par
        downloadLink.href = window.URL.createObjectURL(csvFile);  \par
        downloadLink.style.display = "none";  \par
        document.body.appendChild(downloadLink);  \par
        downloadLink.click();  \par
    \}  \par
    function exportTableToCSV(filename) \{  \par
        var csv = [];  \par
    var x=document.getElementById("srch");\par
    var rows = x.querySelectorAll("table tr");  \par
    for(var i=0; i<rows.length; i++) \{  \par
        var row = [], cols = rows[i].querySelectorAll("td, th");  \par
        for( var j=1; j<cols.length-1; j++)  \par
        row.push(cols[j].innerText);  \par
        csv.push(row.join(","));  \par
    \}   \par
    //call the function to download the CSV file  \par
    downloadCSV(csv.join("\\n"), filename);  \par
    \}  \par
